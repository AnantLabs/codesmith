    <Compile Include="Factory\<%= GetSingularName(ProcedureMap.RecordSets[i].Name) %>Factory.cs" />
	<Compile Include="I<%= GetSingularName(ProcedureMap.RecordSets[i].Name) %>BL.cs" />	
    <Compile Include="I<%= GetPluralName(ProcedureMap.RecordSets[i].Name) %>BL.cs" />	
    <% } %>    
    <Compile Include="Base\Validation\ValidationMessage.cs" />      
    <Compile Include="Base\Validation\ValidationMessages.cs" />  
    <Compile Include="Base\Validation\ValidationLevel.cs">
      <SubType>Code</SubType>
    </Compile>    
