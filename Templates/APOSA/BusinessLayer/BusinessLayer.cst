    <DelaySign>false</DelaySign>
    <OutputType>Library</OutputType>
    <RootNamespace><%= CompanyNameSpace %>.<%= ProjectNameSpace %>.BusinessLayer</RootNamespace>
    <RunPostBuildEvent>OnBuildSuccess</RunPostBuildEvent>
