  <ItemGroup>
    <ProjectReference Include="..\DataLayer\<%= CompanyNameSpace %>.<%= ProjectNameSpace %>.DataLayer.csproj">
      <Project>{377B7B27-D4EA-4176-8F51-5F566B378575}</Project>
      <Name><%= CompanyNameSpace %>.<%= ProjectNameSpace %>.DataLayer</Name>
    </ProjectReference>
