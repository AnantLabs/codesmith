<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NHibVbSample.Generated.BusinessObjects" assembly="NHibVbSample.Data">
	<class name="NHibVbSample.Generated.BusinessObjects.Profile, NHibVbSample.Data" table="dbo.Profiles" lazy="true">
		<id name="Id" column="UniqueID">
		 	<generator class="native" />
		</id>
		
		<property name="Username" column="Username" />
		<property name="ApplicationName" column="ApplicationName" />
		<property name="IsAnonymous" column="IsAnonymous" />
		<property name="LastActivityDate" column="LastActivityDate" />
		<property name="LastUpdatedDate" column="LastUpdatedDate" />
		<bag name="Accounts" lazy="true" cascade="all-delete-orphan"  inverse="true" >
			<key column="UniqueID"></key>
			<one-to-many class="Account"></one-to-many>
		</bag>
		<bag name="Carts" lazy="true" cascade="all-delete-orphan"  inverse="true" >
			<key column="UniqueID"></key>
			<one-to-many class="Cart"></one-to-many>
		</bag>
	</class>
</hibernate-mapping>