        <RunLocal()> _
        Public Function Create() As <%= Entity.Name %>List
            Dim item As <%= Entity.Name %>List = (<%= Entity.Name %>List)Activator.CreateInstance(typeof(<%= Entity.Name %>List), True)
    
            Dim cancel As Boolean = False
